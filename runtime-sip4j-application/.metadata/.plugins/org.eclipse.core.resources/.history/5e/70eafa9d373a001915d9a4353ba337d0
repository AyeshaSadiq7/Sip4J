package jomp.montecarlo2;

public class test {

	Integer a = 10;
	Integer[] array = new Integer[]{1,3,5,7,9};
	
	public void setA(){
		a = 1000;
		array[2] = 90;
	}
	public void createObject(){
		array = new Integer[]{0};
		a = new Integer(300);
	}
	public void readA(){
		System.out.println("a = "+a);
	}

	public static void main(String[] arg){
		test obj = new test();
		obj.setA();
		obj.createObject();
		obj.readA();
		
	}
}

