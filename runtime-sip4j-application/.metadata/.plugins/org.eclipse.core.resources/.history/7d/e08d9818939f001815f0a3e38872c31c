package test;

public class B {
	
	int data = 100;
}

class A{
	
	B x,y,z,w;
	
	public B foo(B z, int a){
		x = w;
		B t = x;
		y =  t;
		a = 15;
		System.out.println(""+w.data+z.data+t.data);
		return y;
		
	}
}

class Client{
	
	public static void main(String[] arg){
		
		A a = new A();
		a.foo(a.z, a.x.data);
	}
}